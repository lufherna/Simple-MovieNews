<script src='app.js'></script>

	<div class = 'jumbotron'>
		<h1>Simple Movie News Welcomes You!</h1>
	</div>
	
	{{#each data}}
	<div class = 'row'>
		<div class = 'col-lg-12'>
			<div class = 'panel panel-primary'>
			<div class="panel-heading">
				<h3 class="panel-title">{{this.title}}</h3>
			</div>
				<div class = 'panel-body'>
				<p> This is some Article title: {{this.title}}</p>
				<p> This is the link: <a href = {{this.link}}>{{this.link}}</a></p>
			{{#each note}}
				<p data-id = {{this.id}}>{{this.title}}</p>
			{{/each}}
				</div>
			</div>
		</div>
	</div>
	{{/each}}
	